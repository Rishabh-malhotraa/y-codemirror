{
  "name": "y-codemirror",
  "version": "1.0.4",
  "description": "CodeMirror binding for Yjs",
  "main": "./dist/y-codemirror.js",
  "types": "./dist/src/y-codemirror.d.ts",
  "module": "./src/y-codemirror.js",
  "sideEffects": false,
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "dist": "rm -rf dist && rollup -c && tsc",
    "watch": "rollup -wc",
    "test": "npm run lint",
    "lint": "standard && tsc",
    "preversion": "npm run lint && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-codemirror.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-codemirror/issues"
  },
  "homepage": "https://github.com/yjs/y-codemirror#readme",
  "peerDependencies": {
    "yjs": "^13.0.0"
  },
  "dependencies": {
    "lib0": "^0.2.20"
  },
  "devDependencies": {
    "rollup": "^2.2.0",
    "standard": "^14.3.3",
    "typescript": "^3.8.3"
  }
}
